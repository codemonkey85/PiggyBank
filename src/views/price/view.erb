<h1>
  <%= "Price '#{@price.price_id}'" %>
</h1>
<div class="links">
  <a href="/prices">Prices Index</a>
</div>
<%= erb :"price/as_dl" %>
<div class="buttons">
  <a class="btn primary" href="/price/<%= @price.price_id %>?edit">
    <span class="icon icon-pencil"></span>
    <span class="text">Edit </span>
  </a>
  <a class="btn danger" href="/price/<%= @price.price_id %>?delete">
    <span class="icon icon-trash"></span>
    <span class="text">Delete</span>
  </a>
</div>
