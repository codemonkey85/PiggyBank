@model IEnumerable<ValidationWarning>
@{
    ViewData["Title"] = "Account Validation";
}

<h1>Account Validation</h1>
@{
    if (Model.Count() == 0)
    {
        <p>All accounts are valid. Nice!</p>
    }
    else
    {
        <ul>
            @foreach(ValidationWarning warning in Model)
            {
            <li>
                Account "@(((Account)warning.Entity).FullName)" is invalid. @(warning.Message)
            </li>
            }
        </ul>
    }
}